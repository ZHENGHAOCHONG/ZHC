<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/shoudong"
    android:padding="15dp"
    android:background="@drawable/newbackg">

    <Button
        android:id="@+id/stop"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="35dp"
        android:background="@drawable/buttonbg2"
        android:text="停止"
        android:textSize="20dp" />

    <Button
        android:id="@+id/AutoBack"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="70dp"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="35dp"
        android:background="@drawable/buttonbg2"
        android:text="返航"
        android:textSize="20dp" />


    <Button
        android:id="@+id/JumpAuto"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="140dp"
        android:layout_marginLeft="160dp"
        android:layout_marginTop="35dp"
        android:background="@drawable/buttonbg2"
        android:text="自动"
        android:textSize="20dp" />

    <Button
        android:id="@+id/HalfAuto"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="210dp"
        android:layout_marginLeft="240dp"
        android:layout_marginTop="35dp"
        android:background="@drawable/buttonbg2"
        android:text="半自动"
        android:textSize="20dp" />

    <Button
        android:id="@+id/TurnToAnother"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="280dp"
        android:layout_marginLeft="240dp"
        android:layout_marginTop="35dp"
        android:background="@drawable/buttonbg2"
        android:text="转塘"
        android:textSize="20dp" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/paopan"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="100dp"
        android:text="抛盘电机转动速度"
        android:textSize="20dp"
        android:textColor="#FFFAFA" />

    <Spinner
        android:id="@+id/spinner1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_toEndOf="@+id/paopan"
        android:layout_toRightOf="@+id/paopan" />



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/zhendong"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="140dp"
        android:text="振动电机振动速度"
        android:textSize="20dp"
        android:textColor="#FFFAFA" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_toEndOf="@+id/zhendong"
        android:layout_toRightOf="@+id/zhendong" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/dangwei"
        android:layout_marginStart="258dp"
        android:layout_marginLeft="258dp"
        android:layout_marginTop="100dp"
        android:text="倒挡"
        android:textSize="20dp"
        android:textColor="#FFFAFA" />

    <CheckBox
        android:id="@+id/CheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:layout_marginStart="300dp"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="100dp" />

    <ImageView
        android:id="@+id/root1"
        android:layout_width="157dp"
        android:layout_height="169dp"
        android:layout_marginStart="100dp"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="170dp"
        android:layout_marginBottom="34dp"
        android:src="@drawable/root5"
        />

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/root"
        android:layout_width="376dp"
        android:layout_height="156dp"
        android:layout_alignTop="@+id/root1"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:orientation="vertical"
        tools:context=".omViewActivity">

    </LinearLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1dip"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="10dp"
        android:background="#FF909090" />

    <View
        android:id="@+id/view"
        android:layout_width="fill_parent"
        android:layout_height="1dip"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="350dp"
        android:background="#FF909090" />

    <!--剩余电量-->
    <TextView
        android:id="@+id/Etric"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view"
        android:layout_marginTop="20dp"
        android:text="剩余电量:"
        android:textSize="20dp"
        android:textColor="#FFFAFA"/>


    <TextView
        android:id="@+id/Etricrev"
        android:layout_width="80dp"
        android:layout_height="25dp"
        android:layout_alignBottom="@+id/Etric"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_toEndOf="@+id/Etric"
        android:layout_toRightOf="@+id/Etric"
        android:background="@drawable/parcel"
        android:gravity="center_horizontal"
        android:text="35"
        android:textSize="18dp"
        android:textColor="#000"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="%"
        android:textSize="20dp"
        android:layout_toRightOf="@id/Etricrev"
        android:layout_marginLeft="5dp"
        android:layout_alignBottom="@id/Etricrev"
        android:textColor="#FFFAFA"
        />

    <!--剩余饵料-->
    <TextView
        android:id="@+id/Erl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Etric"
        android:layout_alignStart="@+id/Etric"
        android:layout_below="@+id/Etric"
        android:layout_marginTop="13dp"
        android:text="剩余饵料:"
        android:textSize="20dp"
        android:textColor="#FFFAFA"
        />

    <TextView
        android:id="@+id/Erlrev"
        android:layout_width="80dp"
        android:layout_height="25dp"
        android:layout_alignBottom="@+id/Erl"
        android:layout_alignLeft="@+id/Etricrev"
        android:layout_alignStart="@+id/Etricrev"
        android:background="@drawable/parcel"
        android:gravity="center_horizontal"
        android:text="44"
        android:textSize="18dp"
        android:textColor="#000"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="kg"
        android:textSize="20dp"
        android:layout_toRightOf="@id/Erlrev"
        android:layout_marginLeft="5dp"
        android:layout_alignBottom="@id/Erlrev"
        android:textColor="#FFFAFA"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Erl"
        android:layout_alignStart="@+id/Erl"
        android:layout_below="@+id/Erl"
        android:layout_marginTop="12dp"
        android:text="当前位置:"
        android:textColor="#FFFAFA"
        android:textSize="20dp"
        android:id="@+id/textView10" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Erlrev"
        android:layout_alignStart="@+id/Erlrev"
        android:layout_below="@+id/Erlrev"
        android:layout_marginTop="12dp"
        android:text="南"
        android:textColor="#FFFAFA"
        android:textSize="20dp"
        android:id="@+id/North" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Erlrev"
        android:layout_alignStart="@+id/Erlrev"
        android:layout_below="@+id/North"
        android:text="东"
        android:textColor="#FFFAFA"
        android:textSize="20dp"
        android:id="@+id/East"/>


    <TextView
        android:id="@+id/textView11"
        android:layout_width="125dp"
        android:layout_height="25dp"
        android:layout_alignBaseline="@+id/textView10"
        android:layout_alignStart="@+id/Erlrev"
        android:layout_alignLeft="@+id/Erlrev"
        android:layout_alignBottom="@+id/North"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginBottom="-66dp"
        android:background="@drawable/parcel"
        android:gravity="center_horizontal"
        android:text="32.19774325"
        android:textColor="#000"
        android:textSize="18dp" />


    <TextView
        android:id="@+id/textView12"
        android:layout_width="125dp"
        android:layout_height="25dp"
        android:layout_marginTop="480dp"
        android:layout_alignLeft="@+id/textView11"
        android:layout_alignStart="@+id/textView11"
        android:layout_alignBottom="@+id/East"
        android:background="@drawable/parcel"
        android:gravity="center_horizontal"
        android:text="119.51684327"
        android:textColor="#000"
        android:textSize="18dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="m"
        android:textSize="20dp"
        android:layout_toRightOf="@id/textView11"
        android:layout_marginLeft="5dp"
        android:layout_alignBottom="@id/North"
        android:textColor="#FFFAFA"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="m"
        android:textSize="20dp"
        android:layout_toRightOf="@id/textView12"
        android:layout_marginLeft="5dp"
        android:layout_alignBottom="@id/East"
        android:textColor="#FFFAFA"
        />


</RelativeLayout>